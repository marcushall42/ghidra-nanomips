    is i_op=0b011000 & rt & i16_20=0b00011 & i0_15 ; e0_15s
    [ imm = inst_next + ((e0_15s<<16)|i0_15); ] { rt = imm; }

# ADDU - Add (Untrapped)  pg. 30.
