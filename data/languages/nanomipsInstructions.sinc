    [ offset = (i16_20<<16)|(i2_15<<2); ] { *:4 (gp+offset) = rt; }
:sw rtz3, offset("gp")
    is n_op=0b110101 & rtz3 & n0_6
    [ offset = (n0_6<<2); ] { *:4 (gp+offset) = rtz3; }
:sw rt, offset(rs)
    is i_op=0b101001 & rt & rs & i15s & i8_14=0b1001000 & i0_7
    [ offset = (i15s<<8)|i0_7; ] { *:4 (rs+offset) = rt; }
