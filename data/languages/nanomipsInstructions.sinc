# MUH - Multiply High  pg. 137.
:muh rd, rs, rt
    is i_op=0b001000 & rt & rs & rd & i0_9=0b0001011000 {
    tmpS:8 = sext(rs);
    tmpT:8 = sext(rt);
    tmpS = tmpS * tmpT;
