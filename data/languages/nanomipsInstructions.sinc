# ALUIPC - Add aLigned Upper Immediate to PC pg. 33.
:aluipc rt, "%pcrel_hi"(imm)
    is i_op=0b111000 & rt & i16_20 & i12_15 & i2_11 & i1=1 & i0s
    [ imm = (inst_next
             + (i0s<<31)|(i2_11<<21)|(i16_20<<15)|(i2_11<<12)) & ~0xfff; ]
    { rt = imm; }

