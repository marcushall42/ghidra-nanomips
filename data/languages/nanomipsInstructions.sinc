    { tempS:8 = zext(i_rs3); tempS = tempS >> shift; i_rt3 = tempS:4; }

# SRLV - Shift Right Logical Variable  pg. 193.
:srlv rd, rs, rt
    is i_op=0b001000 & rt & rs & rd & i0_9=0b0001010000
    { tempS:8 = zext(rs); tempS = tempS >> rt[0,5]; rd = tempS:4; }

# SUB - Subtract  pg. 195.
