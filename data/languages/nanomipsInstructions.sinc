:addiu rt3, rs3, imm
    is n_op=0b100100 & rt3 & rs3 & n3=0 & n0_2
    [ imm = n0_2<<2; ] { rt3 = rs3 + imm; }
# ADDIU - Add Immediate (Untrapped) pg. 28.
:addiu n_rt, imm
# rt!=0 (rt == n5_9)
    is n_op=0b100100 & n_rt & n5_9!=0 & n4s & n3=1 & n0_2
    [ imm = (n4s<<3)|n0_2; ] { n_rt = n_rt + imm; }
